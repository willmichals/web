{
  "name": "web", // Match the CI name from your logs
  "compatibility_date": "2025-12-24",
  "assets": {
    "directory": "./"
  },
  "workers_dev": true,
  "preview_urls": true,
  "routes": [
    { "pattern": "willmichals.com", "custom_domain": true },
    { "pattern": "www.willmichals.com", "custom_domain": true }
  ],
  "observability": {
    "enabled": false,
    "head_sampling_rate": 1,
    "logs": {
      "enabled": true,
      "head_sampling_rate": 1,
      "persist": true,
      "invocation_logs": true
    },
    "traces": {
      "enabled": false,
      "persist": true,
      "head_sampling_rate": 1
    }
  }
}
